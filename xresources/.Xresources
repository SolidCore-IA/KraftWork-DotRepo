! "Window Format. w (desktop name), t (title), n (name), r (role), c (class)" Set from: Default
! rofi.window-format:                  {w}   {c}   {t}
! "Color scheme for row"
! State:                 'bg',       'fg',      'bgalt',    'hlbg',     'hlfg'
rofi.color-normal:      #404552,    #D3DAE3,    #686d79,    #4084D6,    #404552
rofi.color-urgent:      #404552,    #dc322f,    #404552,    #dc322f,    #fdf6e3
rofi.color-active:      #404552,    #4084D6,    #404552,    #268bd2,    #fdf6e3
! "Color scheme window"
!                       'background', 'border', 'separator'
rofi.color-window:       #383C4A,     #4084D6,   #E45E5E
rofi.bw: 2
! "Separator style (none, dash, solid)" Set from: Default
rofi.separator-style: solid
rofi.hide-scrollbar: true
! rofi.scrollbar-width: 8
! Look stuff
rofi.font: Ubuntu-R 12
rofi.width: 50
rofi.lines: 10
rofi.padding: 10
! "Margin between rows" Set from: Default
! rofi.line-margin:                    2
! "Padding within rows" Set from: Default
! rofi.line-padding:                   1
!
! location stuff
!rofi.location: 2
! rofi.yoffset: 0
! rofi.xoffset: 0
!rofi.sidebar-mode: true
!
! "Enabled modi" Set from: Default
rofi.modi: drun,window,ssh
! "Set the modi to combine in combi mode" Set from: Default
! rofi.combi-modi:                     window,run
!rofi.auto-select: false !!FREEZES IN REMOTE SESSION
